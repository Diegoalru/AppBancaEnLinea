<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppBancaEnLinea.Views.TestPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Por motivos de seguridad, vuelva a iniciar la sesión."
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Button Text="Volver" 
                        BorderColor="Navy" 
                        BackgroundColor="Navy" 
                        TextColor="Silver" 
                        CornerRadius="10"
                        Clicked="Resume">
            </Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>